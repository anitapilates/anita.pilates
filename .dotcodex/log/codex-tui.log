+import { NextResponse } from "next/server";
+
+const CHANNEL_HANDLE_URL = "https://www.youtube.com/@Anita.pilates";
+
+const controllerWithTimeout = (ms: number) => {
+  const controller = new AbortController();
+  const timeout = setTimeout(() => controller.abort(), ms);
+  return { controller, timeout };
+};
+
+async function getChannelIdFromHandle(): Promise<string | null> {
+  const { controller, timeout } = controllerWithTimeout(8000);
+  try {
+    const res = await fetch(CHANNEL_HANDLE_URL, {
+      signal: controller.signal,
+      cache: "no-store",
+    });
+    if (!res.ok) throw new Error("channel page fail");
+    const html = await res.text();
+    const match = html.match(/"channelId":"(UC[^"]+)"/);
+    return match ? match[1] : null;
+  } catch {
+    return null;
+  } finally {
+    clearTimeout(timeout);
+  }
+}
+
+async function fetchFeed(channelId: string): Promise<string | null> {
+  const feedUrl = `https://www.youtube.com/feeds/videos.xml?channel_id=${channelId}`;
+  const { controller, timeout } = controllerWithTimeout(8000);
+  try {
+    const res = await fetch(feedUrl, {
+      signal: controller.signal,
+      cache: "no-store",
+    });
+    if (!res.ok) throw new Error("feed fail");
+    return await res.text();
+  } catch {
+    return null;
+  } finally {
+    clearTimeout(timeout);
+  }
+}
+
+function parseLatestFromFeed(xml: string): {
+  videoId: string | null;
+  title: string | null;
+  published: string | null;
+} {
+  try {
+    const idMatch = xml.match(/<yt:videoId>([^<]+)<\/yt:videoId>/);
+    const titleMatch = xml.match(/<title>([^<]+)<\/title>/g);
+    const publishedMatch = xml.match(/<published>([^<]+)<\/published>/);
+    const videoId = idMatch ? idMatch[1] : null;
+    // First <title> is channel title; second is latest video
+    const title =
+      titleMatch && titleMatch.length > 1
+        ? titleMatch[1].replace(/<\/?title>/g, "")
+        : null;
+    const published = publishedMatch ? publishedMatch[1] : null;
+    return { videoId, title, published };
+  } catch {
+    return { videoId: null, title: null, published: null };
+  }
+}
+
+export const revalidate = 1800; // 30 min
+
+export async function GET() {
+  try {
+    const channelId = await getChannelIdFromHandle();
+    if (!channelId) throw new Error("no channel id");
+    const xml = await fetchFeed(channelId);
+    if (!xml) throw new Error("no feed");
+    const latest = parseLatestFromFeed(xml);
+    return NextResponse.json(latest);
+  } catch {
+    return NextResponse.json(
+      { videoId: null, title: null, published: null },
+      { status: 200 }
+    );
+  }
+}
